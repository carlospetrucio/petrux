        <client_script><![CDATA[api.controller = function() {
  /* Widget controller */
  var c = this;

  // Obtém a hora atual
  var currentHour = new Date().getHours();

  // Define a saudação com base na hora do dia
  c.greeting = currentHour < 12 ? 'Bom dia!' : 'Boa tarde!';
};
]]></client_script>
        <controller_as>c</controller_as>
        <css>.footer-widget {
  background-color: #343a40;
  color: white;
  padding: 20px 10px;
  font-size: 14px;
}

.footer-content {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.footer-section {
  flex: 1 1 30%;
  margin: 10px;
}

.footer-section h4 {
  border-bottom: 2px solid #ea680b;
  padding-bottom: 5px;
  margin-bottom: 10px;
}

.footer-section ul {
  list-style: none;
  padding: 0;
}

.footer-section ul li {
  margin: 5px 0;
}

.footer-section ul li a {
  color: #ea680b;
  text-decoration: none;
}

.footer-section ul li a:hover {
  text-decoration: underline;
}

.footer-bottom {
  text-align: center;
  margin-top: 20px;
  border-top: 1px solid #ea680b;
  padding-top: 10px;
}
</css>
